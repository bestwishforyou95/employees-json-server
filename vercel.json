{
  "functions": {
    "api/main.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/main.js"
    }
  ],
  "version": 2,
  "builds": [
    {
      "src": "api/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.js"
    }
  ]
}
